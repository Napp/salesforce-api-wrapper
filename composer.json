{
  "name": "karronoli/salesforce-api",
  "description": "A library for interacting with the Salesforce REST API and managing the OAuth flow",
  "keywords": ["salesforce", "rest", "api", "oauth2"],
  "homepage": "https://github.com/karronoli/salesforce-api-wrapper",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael LundbÃ¸l",
      "email": "ml@napp.dk"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": ">=7.1.3",
    "nesbot/carbon": "^1.12",
    "guzzlehttp/guzzle": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.2",
    "phpro/grumphp": "^0.14.3",
    "wearejust/grumphp-extra-tasks": "^2.2",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-4": {
      "karronoli\\Salesforce\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Karronoli\\Salesforce\\Tests\\": "tests/"
    },
    "files": [
      "tests/bootstrap.php"
    ]
  },
  "minimum-stability": "stable"
}
